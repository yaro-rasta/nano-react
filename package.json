{
	"name": "nanoweb-react",
	"description": "A utility library for React applications.",
	"keywords": [
		"react",
		"hooks",
		"utility"
	],
	"license": "ISC",
	"author": {
		"name": "ЯRаСлав",
		"email": "support@yaro.page",
		"url": "https://yaro.page/i/"
	},
	"private": false,
	"version": "1.3.1",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.cjs",
	"module": "./dist/index.esm.js",
	"exports": {
		".": {
			"input": "./src/index.js",
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.esm.js"
		}
	},
	"scripts": {
		"start": "npm run dev",
		"dev": "vite",
		"build": "rollup -c",
		"lint": "node ../../node_modules/eslint/bin/eslint.js .",
		"preview": "vite preview",
		"release": "node ../../bin/release.js",
		"test": "vitest --run",
		"clean": "node ../../bin/clean.js",
		"ws:check": "node ../../bin/check-workspace.js",
		"ws:workspace": "node ../../bin/check-workspace.js workspace",
		"ws:latest": "node ../../bin/check-workspace.js latest",
		"ws:init": "node ../../bin/check-workspace.js init"
	},
	"dependencies": {
		"markdown-to-jsx": "^7.7.3",
		"react-router-dom": "^6.28.2"
	},
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@babel/preset-env": "^7.26.0",
		"@babel/preset-react": "^7.26.3",
		"@eslint/js": "^9.18.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^26.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"@types/react": "^19.0.7",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react-swc": "^3.7.2",
		"autoprefixer": "^10.4.20",
		"babel-jest": "^29.7.0",
		"eslint": "^9.18.0",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-mock": "^29.7.0",
		"nanoweb-fs": "workspace:*",
		"postcss": "^8.5.1",
		"rollup": "^4.30.1",
		"stylelint": "^16.13.2",
		"stylelint-config-standard": "^36.0.1",
		"supports-color": "^9.4.0",
		"tailwindcss": "^3.4.17",
		"vite": "^5.4.11"
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	}
}
