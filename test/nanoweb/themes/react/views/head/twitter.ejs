	<!-- Twitter meta tags -->
	<meta name="twitter:card" content="summary_large_image">
	<meta name="twitter:title" content="<%= locals.page?.title || 'Page Title' %>">
	<meta name="twitter:description" content="<%= locals.page?.desc || 'Page description' %>">
	<% if (locals.page?.image) { %>
		<meta name="twitter:image" content="<%= locals.page.image %>">
	<% } %>
